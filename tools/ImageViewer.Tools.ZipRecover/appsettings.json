{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Debug"
    }
  },
  "ZipRecovery": {
    "SevenZipPath": "C:\\Program Files\\7-Zip\\7z.exe",
    "TempDirectory": "temp_recovery",
    "BackupDirectory": "backup_corrupted",
    "MaxRetryAttempts": 3,
    "SkipCorruptedFiles": true,
    "ValidateExtractedFiles": true,
    "SkipHealthyArchives": true,
    "HealthCheckTimeoutSeconds": 30,
    "LogLevel": "Debug",
    "ValidateRecoveredArchive": true,
    "ShowBatchWindows": false,
    "SupportedExtensions": [
      ".zip",
      ".7z",
      ".rar",
      ".tar",
      ".gz",
      ".bz2",
      ".xz",
      ".lzma",
      ".cab",
      ".deb",
      ".rpm",
      ".iso",
      ".dmg",
      ".pkg",
      ".msi",
      ".cbz",
      ".cbr",
      ".cbt",
      ".cb7",
      ".arj",
      ".lzh",
      ".lha",
      ".ace",
      ".z",
      ".cpio",
      ".swm",
      ".wim",
      ".esd",
      ".chm",
      ".hfs",
      ".hfsx",
      ".apk",
      ".ipa",
      ".jar",
      ".war",
      ".ear"
    ]
  }
}
