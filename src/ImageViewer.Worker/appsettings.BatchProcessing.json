{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "ImageViewer.Worker.Services.BatchThumbnailGenerationConsumer": "Warning"
    }
  },
  "RabbitMQ": {
    "PrefetchCount": 100,
    "Concurrency": 8,
    "MaxImageSizeBytes": 1073741824,
    "MaxZipEntrySizeBytes": 21474836480,
    "ConnectionRetryDelay": 5000,
    "MaxRetryAttempts": 5
  },
  "BatchProcessing": {
    "MaxBatchSize": 50,
    "BatchTimeoutSeconds": 5,
    "MaxConcurrentBatches": 4
  },
  "MemoryOptimization": {
    "MaxMemoryUsageMB": 40960,
    "MaxConcurrentProcessing": 16,
    "MemoryPoolSize": 100,
    "DefaultBufferSize": 4194304
  },
  "ImageProcessing": {
    "Quality": 85,
    "MaxWidth": 1920,
    "MaxHeight": 1080,
    "SkipExisting": true,
    "BatchSize": 50,
    "UseMemoryStream": true,
    "SkipDiskTemporaryFiles": true,
    "EnableParallelProcessing": true
  }
}
